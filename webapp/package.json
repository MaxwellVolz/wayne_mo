{
  "name": "wayne-mo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "gen:city": "npx gltfjsx@6.5.3 public/models/city_01.glb --output generated_components/CityModelGenerated.tsx --types --keepgroups --keepnames --meta && node scripts/fix-gltf-types.js generated_components/CityModelGenerated.tsx",
    "gen:taxi": "npx gltfjsx@6.5.3 public/models/taxi.glb --output generated_components/TaxiModelGenerated.tsx --types --keepgroups --keepnames --meta && node scripts/fix-gltf-types.js generated_components/TaxiModelGenerated.tsx",
    "gen:tutorial": "npx gltfjsx@6.5.3 public/models/tutorial_01.glb --output generated_components/TutorialModelGenerated.tsx --types --keepgroups --keepnames --meta && node scripts/fix-gltf-types.js generated_components/TutorialModelGenerated.tsx",
    "gen:present-green-cube": "npx gltfjsx@6.5.3 public/models/present_green_cube.glb --output generated_components/present_green_cube.tsx --types --keepgroups --keepnames --meta && node scripts/fix-gltf-types.js generated_components/present_green_cube.tsx",
    "gen:present-green-rectangle": "npx gltfjsx@6.5.3 public/models/present_green_rectangle.glb --output generated_components/present_green_rectangle.tsx --types --keepgroups --keepnames --meta && node scripts/fix-gltf-types.js generated_components/present_green_rectangle.tsx",
    "gen:present-green-round": "npx gltfjsx@6.5.3 public/models/present_green_round.glb --output generated_components/present_green_round.tsx --types --keepgroups --keepnames --meta && node scripts/fix-gltf-types.js generated_components/present_green_round.tsx",
    "gen:presents": "npm run gen:present-green-cube && npm run gen:present-green-rectangle && npm run gen:present-green-round",
    "gen:models": "npm run gen:city && npm run gen:taxi && npm run gen:tutorial",
    "gen:all": "npm run gen:models && npm run gen:presents",
    "deploy": "npm run build && mkdir -p ~/_git/intervolzdotcom/public/crazytaxi && cp -r out/* ~/_git/intervolzdotcom/public/crazytaxi/ && echo 'Deployed to intervolz.com/crazytaxi'"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "lucide-react": "^0.562.0",
    "next": "15.5.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.180.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "typescript": "^5"
  }
}
